.view-homepage-slideshow {
  position: relative;
  .view-header {
    position: absolute;
    z-index: 500;
    left: 50%;
    top: 60%;
    transform: translateX(-50%) translateY(-50%);
    width: 75%;

    .slideshow-button {
     // position: absolute;
	position: relative;

      .hbtn {
        border-radius: 5px;
        padding: 15px 25px;
        font-size: 22px;
        text-decoration: none;
        margin: 20px;
        color: #fff;
        position: relative;
        display: inline-block;
        font-family: FreightSansBlack, "Helvetica Neue", Helvetica;
        letter-spacing: 2px;
      }

      .hbtn:active {
        transform: translate(0px, 5px);
        -webkit-transform: translate(0px, 5px);
        box-shadow: 0px 1px 0px 0px;
      }

      .hblue {
        background-color: #0F457F;
        box-shadow: 0px 5px 0px 0px #0D3B6D;
      }

      .hblue:hover {
        background-color: #12559e;
      }
      .hred {
        background-color: #7f0b08;
        box-shadow: 0px 5px 0px 0px #680907;
      }

      .hred:hover {
        background-color: #9b0d0a;
      }
    }
    .slideshow-button-left {
     // left: 50%;
    //  top: 62px;
    //  transform: translateX(-50%);
	text-align: center; 
    }
    .slideshow-button-right {
      right: 0;
      top: 61px;
      display: none;
    }

    .slideshow-text {
      text-align: center;
      font-family: FreightSansBlack, "Helvetica Neue", Helvetica;
    //  font-family: "Hand-Mocha-Mattari", "Helvetica Neue", Helvetica;
      line-height: 75px;

      .slide1-text {
        font-family: FreightSansBlack, "Helvetica Neue", Helvetica;
        color: white;
        font-size: 30px;
        text-transform: uppercase;
        text-shadow: 3px 2px 12px rgba(0, 0, 0, 0.7);
        margin-bottom: 75px;

        .char1 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(5px) rotate(-5deg);
        }
        .char2 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(4px) rotate(-4deg);
        }
        .char3 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(3px) rotate(-3deg);
        }
        .char4 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(2px) rotate(-2deg);
        }
        .char5 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(1px) rotate(-1deg);
        }
        .char6 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(0px) rotate(0deg);
        }
        .char7 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(0px) rotate(0deg);
        }

        /* */
        .char8 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(0px) rotate(0deg);
        }
        /* */

        .char9 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(1px) rotate(1deg);
        }
        .char10 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(2px) rotate(2deg);
        }
        .char11 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(3px) rotate(3deg);
        }
        .char12 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(4px) rotate(4deg);
        }
        .char13 {
          display: inline-block;
          transition: none 0s ease 0s;
          transform: translateX(0px) translateY(5px) rotate(5deg);
        }
      }
      .slide2-text {
        color: white;
        font-size: 37px;
        text-transform: capitalize;
        text-shadow: 2px 2px 0px black;
      }
    }
  }
}

