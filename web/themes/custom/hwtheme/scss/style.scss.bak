// Default variables. 
@import "default-variables";
// Bootstrap Framework.
@import '../bootstrap/assets/stylesheets/bootstrap';
// Base-theme overrides.
@import 'drupal-bootstrap';

@font-face {
  font-family: 'Hand-Mocha-Mattari';
  font-style: normal;
  font-weight: normal;
  src: url("fonts/hand-mocha-mattari-webfont.woff2") format("woff2"),
  url("fonts/hand-mocha-mattari-webfont.woff") format("woff");
}

@font-face {
  font-family: 'FreightSansBlack';
  src: url('fonts/FreightSansBlack.eot');
  src: url('fonts/FreightSansBlack.eot') format('embedded-opentype'),
  url('fonts/FreightSansBlack.woff2') format('woff2'),
  url('fonts/FreightSansBlack.woff') format('woff'),
  url('fonts/FreightSansBlack.ttf') format('truetype'),
  url('fonts/FreightSansBlack.svg#FreightSansBlack') format('svg');
}

@font-face {
  font-family: 'BentonSans';
  src: url('fonts/BentonSans-Regular.woff2') format('woff2'),
  url('fonts/BentonSans-Regular.woff') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

body {
  margin-top: 0 !important;
  font-family: BentonSans, "Helvetica Neue", Helvetica;
  font-size: 19px;
  font-weight: 400;
  line-height: 33.25px;
  text-rendering: optimizeLegibility;
  -moz-text-size-adjust: 100%;
}

body.toolbar-tray-open header,
body.path-newz header {
  display: none;
}

header {
  background: rgba(255, 255, 255, .3);
  transition: top 1s ease-in-out, background .2s ease-in-out;
  -webkit-transition: top 1s ease-in-out, background .2s ease-in-out;

  .navbar-brand {
    display: none;
  }
  .logo {
    img {
      max-width: 250px;
    }
  }
  nav {
    ul {
      list-style: none;
      position: relative;
      left: 50%;
      transform: translateX(-50%);
      float: left;
      margin-bottom: 0px !important;

      li {
        float: left;
        a {
          font-size: 1.4vw;
          position: relative;
          top: 44px;
          color: #0F457F;
          font-family: FreightSansBlack, "Helvetica Neue", helvetica;
        }
        a:hover {
          color: #597CAB;
          text-decoration: none;
        }
        a.menu-logo {
          top: inherit !important;
        }
        img {
          max-width: 250px !important;
          transition: max-width 1s ease-in-out;
          -webkit-transition: max-width 1s ease-in-out;
        }
        > div > div {
          padding: 0 30px;
        }
      }
    }
  }
  .collapse.in,
  .collapsing {
    border-color: inherit !important;
    background: white;
    nav {
      ul {
        list-style: none;
        position: relative;
        left: 50%;
        transform: translateX(-50%);
        float: left;
        margin-bottom: 0px !important;
        text-align: center;

        li {
          float: none;
          position: relative;
          a {
            top: 0 !important;
            font-size: 20px;
            position: relative;
            color: #0F457F;
            font-family: FreightSansBlack, "Helvetica Neue", helvetica;
          }
          a:hover {
            color: #597CAB;
            text-decoration: none;
          }
          a.menu-logo {
            top: inherit !important;
          }
          img {
            max-width: 250px !important;
            transition: max-width 1s ease-in-out;
            -webkit-transition: max-width 1s ease-in-out;
          }
          > div > div {
            padding: 0 30px;
          }
        }
        li.logo-menu {
          display: none;
        }
        li:after {
          content: " ";
          width: 50%;
          background-color: #A2968A;
          bottom: -7px;
          height: 1px;
          position: absolute;
          left: 50%;
          transform: translateX(-50%);
        }
        li:last-child:after {
          content: none;
        }
      }
    }
  }
}

header:hover {
  background: rgba(255, 255, 255, .9);
  transition: background .2s ease-in-out;
  -webkit-transition: background .2s ease-in-out;
}

/* Fixed navigation animation */

.scrollin-down {
  transition: top .7s ease-in-out;
  -webkit-transition: top .7s ease-in-out;
  top: -160px;
}

.scrollin-up {
  transition: top .6s ease-in-out;
  -webkit-transition: top .6s ease-in-out;
  top: 0;
}

.img-down {
  transition: max-width .5s ease-in-out;
  -webkit-transition: max-width .5s ease-in-out;
  max-width: 0 !important;
}

.img-up {
  transition: max-width 1s ease-in-out;
  -webkit-transition: max-width 1s ease-in-out;
  max-width: 250px !important;
}

body > div.dialog-off-canvas-main-canvas > div > div > div {
  padding-right: 0px !important;
  padding-left: 0px !important;
}

body > div.dialog-off-canvas-main-canvas > div > div > div > div > h1 {
